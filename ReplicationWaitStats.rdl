  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@program_name">
            <Value>=Parameters!program_name.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>declare @cmd varchar(1000)

IF exists(SELECT * FROM sys.dm_xe_sessions WHERE name = 'replication_agent_waits') 
DROP EVENT SESSION replication_agent_waits ON SERVER; 

SET @cmd = '
CREATE EVENT SESSION [replication_agent_waits] ON SERVER  
ADD EVENT sqlos.wait_info( 
    ACTION(sqlserver.session_id) 
    WHERE ([sqlserver].[client_app_name] = ''' + @program_name + ''' AND [duration]&gt;=(0))), 
ADD EVENT sqlos.wait_info_external( 
    ACTION(sqlserver.session_id) 
    WHERE ([sqlserver].[client_app_name] = ''' + @program_name + ''' AND [duration]&gt;=(0))) 
ADD TARGET package0.asynchronous_file_target  
    (SET FILENAME = N''C:\temp\replication_agent_waits.xel'',  
    METADATAFILE = N''C:\temp\replication_agent_waits.xem'', 
	max_file_size=(1000),max_rollover_files=(1))  
WITH (TRACK_CAUSALITY=ON); 
'
exec (@cmd);

ALTER EVENT SESSION replication_agent_waits   
ON SERVER STATE = START;  

WAITFOR DELAY '00:01:00'; 

ALTER EVENT SESSION replication_agent_waits   
ON SERVER STATE = STOP;  

SELECT CAST(event_data as XML) event_data  
INTO #replication_agent_waits_1  
FROM sys.fn_xe_file_target_read_file  
    ('C:\temp\replication_agent_waits*.xel',  
    'C:\temp\replication_agent_waits*.xem',  
    NULL, NULL);  

SELECT  
    event_data.value  
    ('(/event/action[@name=''session_id'']/value)[1]', 'smallint') as session_id,  
    event_data.value   
    ('(/event/data[@name=''wait_type'']/text)[1]', 'varchar(100)') as wait_type,  
    event_data.value   
    ('(/event/data[@name=''duration'']/value)[1]', 'bigint') as duration,  
    event_data.value   
    ('(/event/data[@name=''signal_duration'']/value)[1]', 'bigint') as signal_duration
INTO #replication_agent_waits_2  
FROM #replication_agent_waits_1;  

SELECT session_id,  
    wait_type,  
    SUM(duration) total_duration,  
    SUM(signal_duration) total_signal_duration,  
    COUNT_BIG(*) total_wait_count  
FROM #replication_agent_waits_2  
GROUP BY session_id,  
    wait_type  
ORDER BY session_id,  
    SUM(duration) DESC;  

DROP TABLE #replication_agent_waits_1; 
DROP TABLE #replication_agent_waits_2; 

DROP EVENT SESSION replication_agent_waits ON SERVER; </CommandText>
      </Query>
