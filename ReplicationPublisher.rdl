        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>if OBJECT_ID('sysreplservers') &gt; 0 
select top 1 '[' + srvname + '][' + db_name() + ']' publisher_db
from sysreplservers with (nolock)
else
