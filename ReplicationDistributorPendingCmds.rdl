  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@dist_agent_id">
            <Value>=Parameters!dist_agent_id.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>declare
@publisher sysname
,@publisher_db sysname
,@publication sysname
,@subscriber sysname
,@subscriber_db sysname
,@subscription_type int

select 
@publisher = ps.name
,@publisher_db = publisher_db
,@publication = publication
,@subscriber = ss.name
,@subscriber_db = subscriber_db
,@subscription_type = subscription_type
from MSdistribution_agents a
join sys.servers ps
on a.publisher_id = ps.server_id
join sys.servers ss
on a.subscriber_id = ss.server_id
where id = @dist_agent_id

exec sp_replmonitorsubscriptionpendingcmds  
@publisher 
,@publisher_db 
,@publication 
,@subscriber 
,@subscriber_db 
,@subscription_type </CommandText>
      </Query>
