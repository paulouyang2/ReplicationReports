  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>-- use inputbuffer
-- for some reason sys.sp_replcmds doesn't show on sys.dm_exec_requests 

declare @session_id int, @query varchar(500)

declare @inputbuffer table(
EventType nvarchar(30)
,Parameters smallint
,EventInfo nvarchar(4000) 
)

select @session_id = s.session_id
from sys.dm_exec_sessions s with (nolock)
where s.program_name like 'Repl-LogReader-_-' + db_name() + '-%'
and db_name(s.database_id ) = db_name()

select @query = 'dbcc inputbuffer(' + cast(@session_id as varchar) + ')'

insert @inputbuffer
exec (@query)

select s.session_id
,s.program_name
,db_name(s.database_id ) db_name
,r.start_time
,r.status
,r.command
,r.blocking_session_id
,r.wait_type
,r.wait_time
,r.last_wait_type
,r.cpu_time
,r.logical_reads
,r.writes
,s.last_request_start_time
,case 
when object_name(t.objectid,t.dbid) is null and r.status is not null
then (select EventInfo from @inputbuffer)
else object_name(t.objectid,t.dbid) 
end sql_text
from sys.dm_exec_sessions s with (nolock)
left join sys.dm_exec_requests r with (nolock)
on s.session_id = r. session_id
outer apply sys.dm_exec_sql_text(r.plan_handle) t
where s.program_name like 'Repl-LogReader-_-' + db_name() + '-%'
and db_name(s.database_id ) = db_name()
</CommandText>
      </Query>
