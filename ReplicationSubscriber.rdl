  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>select 
'[' + a.publisher + ']' 
+ '[' + a.publisher_db + ']'
+ '[' + a.publication + ']'
+ '[' + @@servername + ']'
+ '[' + db_name() + ']'  subscriptions
,left(a.publisher, 15)
+ '_' + left(a.publisher_db,16)
+ case when a.publication = 'ALL' then '' else '_' + left(a.publication,16) end program_name
,db_name() database_name
from MSsubscription_agents a with (nolock)
order by subscriptions</CommandText>
      </Query>
